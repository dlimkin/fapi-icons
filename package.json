{
  "name": "fapi-icons",
  "description": "Adaptation Font Awesome 4.7 under PRIME icons",
  "version": "4.7.1",
  "scripts": {
    "sass": "npm run sass:build && npm run sass:compressed",
    "sass:build": "node-sass --source-map true -o css/ scss/fapi-icons.scss",
    "sass:compressed": "node-sass --output-style compressed scss/fapi-icons.scss css/fapi-icons.min.css"
  },
  "style": "css/fapi-icons.css",
  "keywords": [
    "font",
    "awesome",
    "fontawesome",
    "icon",
    "bootstrap",
    "prime",
    "primeng",
    "faicons",
    "primefaces"
  ],
  "homepage": "http://fontawesome.io/",
  "bugs": {
    "url": "http://github.com/dlimkin/fapi-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlimkin/fapi-icons.git"
  },
  "license": "MIT",
  "dependencies": {
  },
  "engines": {
    "node": ">=0.10.3"
  },
  "devDependencies": {
    "node-sass": "^4.9.3"
  }
}
